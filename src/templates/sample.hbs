
{{#each this}}
  <a class="photo-card" href="{{largeImageURL}}">
    <img src="{{webformatURL}}" class ="web-url-img"alt="{{tags}}" loading="lazy" />
    <div class="info">
        <p class="info-item">
          <b>Likes</b>
          <br>{{likes}}
        </p>
        <p class="info-item">
          <b>Views</b>
          <br>{{views}}
        </p>
        <p class="info-item">
          <b>Comments</b>
          <br>{{comments}}
        </p>
        <p class="info-item">
          <b>Downloads</b>
          <br>{{downloads}}
        </p>
      </div>
  </a>

{{/each}}

